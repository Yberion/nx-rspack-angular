{
  "name": "rspack-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/rspack-angular/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@ng-rspack/nx:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "../../dist/apps/rspack-angular",
        "index": "./src\\index.html",
        "polyfills": ["zone.js"],
        "tsConfig": "./tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["./public"],
        "styles": ["./src\\styles.scss"],
        "main": "./src\\main.ts"
      },
      "configurations": {
        "production": {
          "mode": "production"
        },
        "development": {
          "mode": "development"
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@ng-rspack/nx:serve",
      "options": {
        "port": 4200
      },
      "configurations": {
        "production": {
          "buildTarget": "rspack-angular:build:production"
        },
        "development": {
          "buildTarget": "rspack-angular:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "rspack-angular:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "rspack-angular:build",
        "port": 4200,
        "staticFilePath": "dist/apps/rspack-angular",
        "spa": true
      }
    }
  }
}
